{
  "name": "starter-parcel",
  "version": "1.0.0",
  "description": "parcel boilerplate",
  "main": "./docs/index.html",
  "keywords": [
    "parcel",
    "boilerplate"
  ],
  "author": "7ylee",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fe-grinder/starter-parcel.git"
  },
